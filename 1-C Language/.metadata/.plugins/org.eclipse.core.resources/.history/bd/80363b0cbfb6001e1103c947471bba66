# include <stdio.h>

void reverse_array(int size, int arr[]){

	for(int i = size-1 ; i>=0; i--){
		printf("%d\n",arr[i]);
	}


}


int main(){
	int arr[] = {1,2,3,4,5,6,7};
	reverse_array(7,arr);
}

