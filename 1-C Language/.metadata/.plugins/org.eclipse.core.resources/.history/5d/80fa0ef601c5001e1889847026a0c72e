#include <stdio.h>
#include <stdlib.h>

void swap(char **x,char** y ){

	char *t = *x ;
	x  = y ;
	y = t ;
}

int main()
{
	char *s = "Ahemd";
	char *z = "Hekal";
	swap(s,z);
	printf("%s",s);
//    int arr [6]= {5,10,15,20,25,30};
