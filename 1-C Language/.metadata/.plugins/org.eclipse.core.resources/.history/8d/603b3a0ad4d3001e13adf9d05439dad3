#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

typedef struct {
	uint32_t Id;
	uint8_t Name[10];
	float Degree ; 
	float Total_degree;
}Data_t;


typedef struct Student {
	struct Student *PreviousNode ;
	Data_t student;
	struct Student *NextNode ;
}Node_t;



typedef enum {
	Linked_List_Error;
	Linked_List_Empty;
	Linked_List_Full;
}Linked_Error;




