#include <stdint.h>

// define DataType needed

#define BUFFERTYPE   unsigned int


// define LIFO

typedef struct {
	uint8_t counter ,
	BUFFERTYPE *base,
	BUFFERTYPE *head
}LIFO_buffer_t;


